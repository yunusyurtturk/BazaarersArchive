{{>user-profile-pic-and-info}}



<!--  User Items Colums -->
<div class="col-md-9 col-sm-9 col-xs-12" style="margin-top: 2em;">
    <div class="row">
        <div class="col-lg-12">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#" onclick="getUserItems({{userid}}, $('#list-container'));">Items</a></li>
                <li><a data-toggle="tab" href="#" onclick="getUserFollowers({{userid}}, $('#list-container'));">Followers</a></li>
                <li><a data-toggle="tab" href="#" onclick="getUserFollowings({{userid}}, $('#list-container'));">Followings</a></li>
            </ul>
        </div>
        <div id="listContainer" class="col-lg-12">
            <div id="list-container" class="masonry-container">
                {{#items}}
                    {{>item-in-list}}
                {{/items}}
                {{^items}}
                    {{no-item-message}}
                {{/items}}
                <div style="clear: both"></div>
            </div>
        </div>


    </div>
</div>
